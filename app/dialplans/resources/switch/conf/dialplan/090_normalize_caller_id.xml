<context name="{v_context}">
	<extension name="normalize_caller_id" number="" continue="true" app_uuid="dc6a4865-5f69-4801-b235-fee17222a7ee" order="90">
		<condition field="${call_direction}" expression="outbound"/>
		<condition field="${loopback_leg}" expression="^$"/>
		<condition field="${outbound_caller_id_number}" expression="^\+?1?(\d{10})$" break="never">
			<action application="set" data="outbound_caller_id_number=$1" inline="true"/>
		</condition>
	</extension>
</context>
